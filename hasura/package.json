  "license": "MIT",
  "scripts": {
    "hasura:cli": "./node_modules/hasura-cli/hasura",
    "hasura:init": "npm run hasura:cli -- init",
    "hasura:migrate:create": "npm run hasura:cli -- migrate create \"init\"",
    "hasura:migrate:apply": "npm run hasura:cli -- migrate apply --skip-update-check",
    "hasura:metadata:export": "npm run hasura:cli -- metadata export --skip-update-check",
    "hasura:console": "npm run hasura:cli -- console --skip-update-check"
  },
  "devDependencies": {
    "hasura-cli": "1.3.2"
  }
}
