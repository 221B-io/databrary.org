    "@types/lodash": "^4.14.152",
    "@types/mime-types": "^2.1.0",
    "@types/minio": "^7.0.5",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/pollyjs__adapter-node-http": "^2.0.0",
    "@types/pollyjs__core": "^4.3.0",
    "@types/pollyjs__persister-fs": "^2.0.0",
