    "@nestjs/core": "^7.0.0",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "bull": "^3.14.0",
    "class-transformer": "^0.2.3",
    "connect-redis": "^4.0.4",
    "cross-fetch": "^3.0.4",
    "express-http-proxy": "^1.6.0",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.0",
    "keycloak-admin": "^1.13.0",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "minio": "^7.0.16",
    "nestjs-redis": "^1.2.7",
    "nestjs-session": "^1.0.1",
    "passport": "^0.4.1",
    "passport-keycloak-oauth2-oidc": "^1.0.3",
    "react": "^16.13.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
